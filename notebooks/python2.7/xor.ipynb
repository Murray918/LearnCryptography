    "text = raw_input(\"Enter text: \")\n",
    "key = raw_input(\"Enter key: \")\n",
    "n = len(text)\n",
    "\n",
    "cipher = \"\"\n",
    "for i in range(n):\n",
    "  t = text[i]\n",
    "  k = key[i%len(key)]\n",
    "  x = ord(k) ^ ord(t)\n",
    "  cipher += chr(x)\n",
    "print text, key, cipher.encode(\"hex\")"
   ]
